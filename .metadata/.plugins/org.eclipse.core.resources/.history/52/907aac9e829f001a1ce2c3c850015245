package classe;

public class Jantar {

	public static void main(String[] args) {
		
		Pessoa p1 = new Pessoa();
		Comida c1 = new Comida();
		
		p1.nome = "Felipe";
		p1.peso = 80.0;
		String nomeDaComida = c1.nome = "Carne";
		c1.pesoComida = 0.500;
		
		double pesoJantar = p1.Comer(c1);
		double pesoAtual = pesoJantar + p1.peso;
		System.out.printf("O peso após comer $s é: %.2f Kg", nomeDaComida, pesoAtual);
	}

}
