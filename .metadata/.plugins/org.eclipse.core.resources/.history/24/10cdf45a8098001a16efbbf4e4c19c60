package fundamentos;

public class DesafioAritmetico {

	public static void main(String[] args) {
		
		int a;
		int b;
		int c;
		int d;
		int resultadoA;
		int resultadoB;
		int resultadoLadoE;
		int resultadoLadoD;
		int total;
		
		a = (6 * (3 + 2));
		b = 3 * 2;
		c = (1 - 5 * (2 - 7));
		d = 2;
		
		resultadoA = (int) Math.pow(a, 2);
		resultadoB = (int) Math.pow(b, 2);
		resultadoLadoE = a / b;
		System.out.println(resultadoA);
		System.out.println(resultadoB);
		

	}

}
