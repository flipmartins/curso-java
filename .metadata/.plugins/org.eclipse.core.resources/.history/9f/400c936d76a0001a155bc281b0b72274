package array;

import java.util.Arrays;

public class Exercicio {

	public static void main(String[] args) {
		
		double[] notasAlunosA = new double[4];
		System.out.println(Arrays.toString(notasAlunosA));

		notasAlunosA[0] = 7.9;
		notasAlunosA[0] = 8;
		notasAlunosA[0] = 6.7;
		notasAlunosA[0] = 9.7;
		
		System.out.println(Arrays.toString(notasAlunosA));
		System.out.println(notasAlunosA[0]);
		
	}

}
