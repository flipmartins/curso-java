package classe;

public class DataTeste {

	public static void main(String[] args) {
		
		Data data1 = new Data();
		data1.dia = 11;
		data1.mes = 02;
		data1.ano = 1987;
		
		Data data2 = new Data();
		data2.dia = 28;
		data2.mes = 10;
		data2.ano = 2008;
		
		System.out.println("Data: " + data1.dia + "/" + data1.mes + "/" + data1.ano);
		System.out.println("Data: " + data2.dia + "/" + data2.mes + "/" + data2.ano);

	}

}
