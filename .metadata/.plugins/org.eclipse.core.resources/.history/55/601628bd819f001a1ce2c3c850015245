package classe;

public class Jantar {

	public static void main(String[] args) {
		
		Pessoa p1 = new Pessoa();
		Comida c1 = new Comida();
		
		p1.nome = "Felipe";
		p1.peso = 80.0;
		c1.nome = "Carne";
		c1.pesoComida = 0.500;
		
		double pesoAtual = p1.Comer(c1);
		System.out.printf("O peso após comer é: %.2f Kg", p1.peso + pesoAtual);
	}

}
